var numArray = [1,2,3,4,-9,];


function isLastEvenOdd(numArray) {
    var last = numArray.slice(-1)[0];
    if (last % 2 === 0) {
        return('even');        
    } else {
        return('odd');
    }
}

isLastEvenOdd(numArray);

